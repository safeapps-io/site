{
  "name": "safeapps-site",
  "version": "13.06.2021v3",
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev --p 3001",
    "type": "tsc --noEmit && svelte-check",
    "build:common": "rm -rf ./build && find . -name .DS_Store | xargs rm",
    "build:stage": "yarn build:common && STAGE=yep svelte-kit build",
    "build:prod": "yarn build:common && svelte-kit build"
  },
  "dependencies": {
    "blobs": "2.2.1-beta.0",
    "bulma": "^0.9.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^1.0.10",
    "autoprefixer": "^10.2.1",
    "cssnano": "^4.1.10",
    "dotenv-flow": "^3.2.0",
    "mdsvex": "^0.8.9",
    "postcss": "^8.2.4",
    "postcss-100vh-fix": "^1.0.2",
    "postcss-easing-gradients": "^3.0.1",
    "postcss-load-config": "^3.0.0",
    "sass": "^1.32.4",
    "sass-mq": "^5.0.1",
    "svelte": "^3.17.3",
    "svelte-check": "^1.1.27",
    "svelte-css-vars": "^0.0.1",
    "svelte-i18n": "^3.3.0",
    "svelte-preprocess": "^4.6.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "vite": "^2.1.3"
  },
  "browserslist": "> 0.5%, not ie > 0, not OperaMini all, not Opera > 0, not dead",
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "arrowParens": "avoid",
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "svelteSortOrder": "scripts-options-markup-styles",
    "svelteStrictMode": false,
    "svelteBracketNewLine": false,
    "svelteAllowShorthand": true,
    "svelteIndentScriptAndStyle": true
  }
}
